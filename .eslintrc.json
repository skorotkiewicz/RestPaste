{
  "env": {
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-empty": "off",
    "no-warning-comments": [
      "warn",
      { "terms": ["fixme", "xxx", "todo"], "location": "anywhere" }
    ]
  }
}
